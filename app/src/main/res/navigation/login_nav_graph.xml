<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_nav_graph"
    app:startDestination="@id/loginFragment2">

    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.example.nuru.Fragment.LoginFragment"
        android:label="fragment_login2"
        tools:layout="@layout/fragment_login2" >
        <action
            android:id="@+id/action_loginFragment2_to_signupFragment"
            app:destination="@id/signupFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_mapsActivity"
            app:destination="@id/mapsActivity" />
        <action
            android:id="@+id/action_loginFragment2_to_myPageActivity"
            app:destination="@id/myPageActivity" />
    </fragment>
    <fragment
        android:id="@+id/signupFragment"
        android:name="com.example.nuru.Fragment.SignupFragment"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" >
        <action
            android:id="@+id/action_signupFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
    </fragment>
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.nuru.MapsActivity"
        android:label="activity_maps"
        tools:layout="@layout/activity_maps" />
    <activity
        android:id="@+id/myPageActivity"
        android:name="com.example.nuru.MyPageActivity"
        android:label="activity_my_page"
        tools:layout="@layout/activity_my_page" />
</navigation>